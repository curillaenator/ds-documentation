{
  "compilerOptions": {
    "baseUrl": "./",
    "strictNullChecks": true,
    "downlevelIteration": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noEmit": true,
    "plugins": [
      {
        "name": "typescript-styled-plugin",
        "validate": false
      }
    ],
    "skipLibCheck": true,
    "strict": false,
    "target": "esnext",
    "pretty": true,
    "resolveJsonModule": true
  }
}
