{
  "name": "@pcbl/fe-scripts",
  "version": "2.9.2",
  "description": "Tooling для автоматизации разработки",
  "license": "UNLICENSED",
  "bin": {
    "pcbl-fe": "bin/pcbl-fe.js"
  },
  "publishConfig": {
    "access": "public",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "type": "git",
    "registry": "https://nexus-dev.pcbltools.ru/repository/npm-public/"
  },
  "scripts": {
    "build": "run -T rimraf build && yarn build:raw",
    "build:raw": "run -T tsc -p tsconfig.build.json && cp -R src/commands/*.sh build/src/commands",
    "prepublishOnly": "echo noop",
    "pack": "run -T pcbl-fe pack",
    "checkDep": "run -T pcbl-fe checkDependencies",
    "revertPack": "run -T pcbl-fe revertPack",
    "clean": "run -T pcbl-fe clean"
  },
  "dependencies": {
    "@svgr/core": "^6.2.1",
    "@svgr/plugin-svgo": "6.2.0",
    "chalk": "^4.1.2",
    "cli-progress": "^3.9.0",
    "commander": "2.20.3",
    "cross-spawn": "^6.0.5",
    "crypto": "1.0.1",
    "execa": "^5.0.1",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.4",
    "img-diff-js": "^0.5.2",
    "inquirer": "^7.3.3",
    "jimp": "^0.16.1",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.21",
    "react-dev-utils": "^8.0.0",
    "replace-in-file": "^6.2.0",
    "semver": "^7.3.5",
    "sharp": "^0.28.3",
    "ts-node": "^8.6.2"
  },
  "devDependencies": {
    "@types/cli-progress": "3.9.1",
    "@types/fs-extra": "^9.0.1",
    "@types/globby": "^9.1.0",
    "@types/klaw-sync": "^6.0.0",
    "@types/react-dev-utils": "^9.0.4",
    "@types/sharp": "0.28.3"
  },
  "files": [
    "build"
  ]
}
